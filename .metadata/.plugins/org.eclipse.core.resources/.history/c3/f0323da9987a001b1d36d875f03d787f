package area_trapezio;

public class area_trapezio {
	
public static void main(String[] args) {
		
	    //declaração e inicialização das variáveis.
		float B = 5566757f;
		float b = 2.5676767f;
		float h = 5f;
		float area = ( (B + b) * h ) /2;
		
		System.out.print(area);
		System.out.println(" é a área do trapézio");
		
        }

}